{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "impliedStrict": true,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "rules": {
    // "quotes": ["error", "double"],
    "semi": ["error", "never"]
  }
}